{
  "name": "meal-planning",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "npx tailwindcss -i ./wwwroot/css/input.css -o ./wwwroot/css/site.css --minify",
    "build:react": "webpack --mode production",
    "dev:react": "webpack --mode development --watch",
    "build": "npm run build:css && npm run build:react",
    "watch": "npx tailwindcss -i ./wwwroot/css/site.css -o ./wwwroot/css/output.css --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
